ALTER TABLE nonredundent ADD CONSTRAINT nonredundent_A_nonredundent_G FOREIGN KEY (A) REFERENCES nonredundent(G) ON DELETE CASCADE ON UPDATE CASCADE;
ALTER TABLE nonredundent ADD CONSTRAINT nonredundent_G_nonredundent_A FOREIGN KEY (G) REFERENCES nonredundent(A) ON DELETE SET DEFAULT ON UPDATE CASCADE;
ALTER TABLE nonredundent ADD CONSTRAINT nonredundent_C_nonredundent_C FOREIGN KEY (C) REFERENCES nonredundent(C) ON DELETE CASCADE ON UPDATE CASCADE;
ALTER TABLE nonredundent ADD CONSTRAINT nonredundent_T_nonredundent_G FOREIGN KEY (T) REFERENCES nonredundent(G) ON DELETE CASCADE ON UPDATE CASCADE;
ALTER TABLE RNA ADD CONSTRAINT RNA_TAA_RNA_TA... FOREIGN KEY (TAA) REFERENCES RNA(TA...) ON DELETE CASCADE ON UPDATE CASCADE;
