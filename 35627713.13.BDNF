DROP TABLE IF EXISTS nonredundent;
DROP TABLE IF EXISTS RNA;


CREATE TABLE nonredundent (
A VARCHAR(8) PRIMARY KEY AUTO_INCREMENT NOT NULL UNIQUE DEFAULT 'A',
G VARCHAR(8) AUTO_INCREMENT UNIQUE DEFAULT 'G',
C VARCHAR(8) AUTO_INCREMENT UNIQUE DEFAULT 'C',
T VARCHAR(8) AUTO_INCREMENT UNIQUE DEFAULT 'T')
COMMENT = 'fna';

CREATE TABLE RNA (
TA... REAL(8) NOT NULL DEFAULT    1 >NM_170731.5 BDNF [organism=Homo
      sapiens] [GeneID=627] [transcri
     pt=3]                           
   2 GATAAGCAACAAGTTCCCCAGCGGTCTTCCCG
     CCCTAGCCTGACAAGGCGAAGGTTTTCTTACC
     TGGCGA                          
                   1,1           Top,
TAA REAL(8) PRIMARY KEY AUTO_INCREMENT NOT NULL UNIQUE DEFAULT 64,
